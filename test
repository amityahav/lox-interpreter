class A {
  method() {
    super.method();
    this.second = "second";
    print "A method";
    print x;
  }
}

class B < A {
  method() {
    print "B method";
  }

  test() {
    this.first = "first";
    super.method();
  }
}

var x = "global dec after";

class C < B {
    test2() {
        super.method();
    }
}

var c = C();
c.test();
//c.test2();
